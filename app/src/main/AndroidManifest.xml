<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="alyssajinnel.libed.com.foodgetspinner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".viewAct"/>
        <activity android:name=".dinerList"/>
        <activity android:name=".AddDiner"/>
        <activity android:name=".ChooseFood"/>
        <activity android:name=".ChoosePlace"/>
        <activity android:name=".Results"/>
        <activity android:name=".ChooseBudget"/>
        <activity android:name=".Spinner"/>
    </application>

</manifest>